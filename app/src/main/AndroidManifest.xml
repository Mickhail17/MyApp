<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mika.invul.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp">

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activity2"
            android:label="Activity Two"
            android:parentActivityName=".MainActivity">

        </activity>
        <activity
            android:name=".Activity3"
            android:label="Activity Three"
            android:parentActivityName=".Activity2">

        </activity>

        <activity
            android:name=".Activity4"
            android:label="Activity Four"
            android:parentActivityName=".Activity2">

        </activity>

        <activity
            android:name=".Activity5"
            android:label="Activity Five"
            android:parentActivityName=".Activity2" >

        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>